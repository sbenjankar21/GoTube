<p> <%= video.title%></p>
<p> Ratings count: <%= video.totalStars/5%></p>
<p> Stars: <%= video.stars%> / <%= video.totalStars%></p>
<p> Average rating: <%= video.averageStars%> stars</p>
<p> Date posted <%= video.datePosted%></p>
 <a href = "<%= video.link%>" target="_blank">
    <div class = "item">
      <img class = "img" src="<%=video.imageAddress%>" alt="Lamp" width="50">
      <p class = "vInfo" id = "overflo"> <%= video.title%><br> <%= video.author%></p>
    </div>
</a> 


<form action ="/videos/<%=video.id%>/new-like" method="POST">

  <button type="submit">Like</button>
</form>

<br>
<% reviews.forEach(review => { %>
  <!-- <a href = "/reviews/<%=review.id%>"><div><%=review.title%></div></a> -->
  <!-- <a href = "<%= review.link%>" target="_blank"> -->
  <a href = "/reviews/<%=review.id%>" target="_blank">
      <div class = "item">
        <!-- <img class = "img" src="<%=review.video.imageAddress%>" alt="Lamp" width="50"> -->
        <p class = "vInfo" id = "overflo"> <%= review.content%><br> <%= review.rating%> stars</p>
      </div>
  </a> 
<% }) %>